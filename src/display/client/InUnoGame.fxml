<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" prefHeight="750.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="display.client.InGameController">
    <children>
        <ImageView fx:id="imageViewBG" fitHeight="750.0" fitWidth="1200.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../Test/uno.jpg" />
            </image>
        </ImageView>
        <Button fx:id="placeButton" layoutX="45.0" layoutY="400.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="150.0" style="-fx-background-color: transparent;" />
        <Button fx:id="drawButton" layoutX="45.0" layoutY="500.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="150.0" style="-fx-background-color: transparent;" />
        <Button fx:id="unoButton" layoutX="45.0" layoutY="600.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="150.0" style="-fx-background-color: transparent;" />
        <ImageView fitHeight="230.0" fitWidth="160.0" layoutX="950.0" layoutY="-30.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../pic/card_all/back.jpg" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
